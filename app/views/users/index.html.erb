<table>
  <% @users.each do |user| %>
  <tr class="users">
        <th><%= user.id %></th>
        <th><%= user.name %></th>
        <th><%= user.email %></th>
        <th><%= link_to "Изменить", edit_user_path(user.id) %></th>
        <th><%= link_to "Удалить", user_path(user.id), method: :delete, data: {confirm: "Вы уверены?"} %></th>
  </tr>
  <% end %>
